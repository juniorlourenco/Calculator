{
  "expo": {
    "name": "SimpleCalculator",
    "slug": "calculator",
    "primaryColor": "#F45F0B",
    "description": "calculadora",
    "platforms": [
      "android"
    ],
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#F45F0B"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },

    "android": {
      "package": "com.jldev.simplecalculator",
      "adaptiveIcon": {
        "backgroundColor": "#3861b1",
        "foregroundImage": "./assets/icon.png"
      }
    }
  }
}
